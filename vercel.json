{
  "version": 2,
  "installCommand": "sh vercel-install.sh",
  "buildCommand": "export PATH=\"$HOME/.cargo/bin:$PATH\" && ./trunk_bin build --release",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/public/:path*", "destination": "/public/:path*" },
    { "source": "/(.*).(js|wasm|css|png|jpg|svg|ico|json)", "destination": "/$1.$2" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}